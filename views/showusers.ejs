<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>all users</title>
    <style>table,th,td{
        border: 1px solid black;
        
    }</style>
</head>
<body>
    <table>
        <tr>
            <th>id</th>
            <th>email</th>
            <th>username</th>
        </tr>
        <% for(user of users) { %>
    <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.username %></td>
        <td>
            <form method="GET" action="/user/<%= user.id %>/edit">
            <button>Edit username</button>
        </form>
        </td>
    
    </tr>
<% } %>
    </table>
    
</body>
</html>

